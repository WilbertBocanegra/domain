#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bocanegra/Dev/SvelteKit/domain/node_modules/.pnpm/tsm@2.2.1/node_modules/tsm/node_modules:/home/bocanegra/Dev/SvelteKit/domain/node_modules/.pnpm/tsm@2.2.1/node_modules:/home/bocanegra/Dev/SvelteKit/domain/node_modules/.pnpm/node_modules:/home/bocanegra/Dev/SvelteKit/domain/node_modules:/home/bocanegra/Dev/SvelteKit/node_modules:/home/bocanegra/Dev/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Dev/SvelteKit/domain/back/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bocanegra/Dev/SvelteKit/domain/node_modules/.pnpm/tsm@2.2.1/node_modules/tsm/node_modules:/home/bocanegra/Dev/SvelteKit/domain/node_modules/.pnpm/tsm@2.2.1/node_modules:/home/bocanegra/Dev/SvelteKit/domain/node_modules/.pnpm/node_modules:/home/bocanegra/Dev/SvelteKit/domain/node_modules:/home/bocanegra/Dev/SvelteKit/node_modules:/home/bocanegra/Dev/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Dev/SvelteKit/domain/back/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/tsm@2.2.1/node_modules/tsm/bin.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/tsm@2.2.1/node_modules/tsm/bin.js" "$@"
fi
